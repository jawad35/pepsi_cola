stock) return;\n\n    dispatch(addItemToCart(id, newQty));\n  };\n\n  const decreaseQty = (id, quantity) => {\n    const newQty = quantity - 1;\n\n    if (newQty <= 0) return;\n\n    dispatch(addItemToCart(id, newQty));\n  };\n\n  const checkoutHandler = () => {\n    history.push(\"/login?redirect=shipping\");\n  };\n\n  return (\n    <Fragment>\n      <MetaData title={\"Your Cart\"} />\n      {cartItems.length === 0 ? (\n        <h2 style={{ marginTop: \"200px\" }}>Your Cart is Empty</h2>\n      ) : (\n        <Fragment>\n          <h2 style={{ marginTop: \"200px\" }}>\n            Your Cart: <b>{cartItems.length} items</b>\n          </h2>\n\n          <div className=\"row d-flex justify-content-between\">\n            <div className=\"col-12 col-lg-8\">\n              {cartItems.map((item) => (\n                <Fragment>\n                  <hr />\n\n                  <div className=\"cart-item\" key={item.product}>\n                    <div className=\"row\">\n                      <div className=\"col-4 col-lg-3\">\n                        <img\n                          src={item.image}\n                          alt=\"Laptop\"\n                          height=\"90\"\n                          width=\"115\"\n                        />\n                      </div>\n\n                      <div className=\"col-5 col-lg-3\">\n                        <Link to={`/products/${item.product}`}>\n                          {item.name}\n                        </Link>\n                      </div>\n\n                      <div className=\"col-4 col-lg-2 mt-4 mt-lg-0\">\n                        <p id=\"card_item_price\">${item.price}</p>\n                      </div>\n\n                      <div className=\"col-4 col-lg-3 mt-4 mt-lg-0\">\n                        <div className=\"stockCounter d-inline\">\n                          <span\n                            className=\"btn btn-danger minus\"\n                            onClick={() =>\n                              decreaseQty(item.product, item.quantity)\n                            }\n                          >\n                            -\n                          </span>\n\n                          <input\n                            type=\"number\"\n                            className=\"form-control count d-inline\"\n                            value={item.quantity}\n                            readOnly\n                          />\n\n                          <span\n                            className=\"btn btn-primary plus\"\n                            onClick={() =>\n                              increaseQty(\n                                item.product,\n                                item.quantity,\n                                item.stock\n                              )\n                            }\n                          >\n                            +\n                          </span>\n                        </div>\n                      </div>\n\n                      <div className=\"col-4 col-lg-1 mt-4 mt-lg-0\">\n                        <i\n                          id=\"delete_cart_item\"\n                          className=\"fa fa-trash btn btn-danger\"\n                          onClick={() => removeCartItemHandler(item.product)}\n                        ></i>\n                      </div>\n                    </div>\n                  </div>\n                  <hr />\n                </Fragment>\n              ))}\n            </div>\n\n            <div className=\"col-12 col-lg-3 my-4\">\n              <div id=\"order_summary\">\n                <h4>Order Summary</h4>\n                <hr />\n                <p>\n                  Subtotal:{\" \"}\n                  <span className=\"order-summary-values\">\n                    {cartItems.reduce(\n                      (acc, item) => acc + Number(item.quantity),\n                      0\n                    )}{\" \"}\n                    (Units)\n                  </span>\n                </p>\n                <p>\n                  Est. total:{\" \"}\n                  <span className=\"order-summary-values\">\n                    $\n                    {cartItems\n                      .reduce(\n                        (acc, item) => acc + item.quantity * item.price,\n                        0\n                      )\n                      .toFixed(2)}\n                  </span>\n                </p>\n\n                <hr />\n                <button\n                  id=\"checkout_btn\"\n                  className=\"btn btn-primary btn-block\"\n                  onClick={checkoutHandler}\n                >\n                  Check out\n                </button>\n              </div>\n            </div>\n          </div>\n        </Fragment>\n      )}\n    </Fragment>\n  );\n};\n\nexport default Cart;\n","/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/order/OrderDetails.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/user/UpdateProfile.js",["523"],"import React, { Fragment, useState, useEffect } from 'react'\n\nimport MetaData from '../layout/MetaData'\n\nimport { useAlert } from 'react-alert'\nimport { useDispatch, useSelector } from 'react-redux'\nimport { updateProfile, loadUser, clearErrors } from '../../actions/userActions'\nimport { UPDATE_PROFILE_RESET } from '../../constants/userConstants'\n\nconst UpdateProfile = ({ history }) => {\n\n    const [name, setName] = useState('')\n    const [email, setEmail] = useState('')\n    const [avatar, setAvatar] = useState('')\n    const [avatarPreview, setAvatarPreview] = useState('/images/default_avatar.jpg')\n\n    const alert = useAlert();\n    const dispatch = useDispatch();\n\n    const { user } = useSelector(state => state.auth);\n    const { error, isUpdated, loading } = useSelector(state => state.user)\n\n    useEffect(() => {\n\n        if (user) {\n            setName(user.name);\n            setEmail(user.email);\n            setAvatarPreview(user.avatar.url)\n        }\n\n        if (error) {\n            alert.error(error);\n            dispatch(clearErrors());\n        }\n\n        if (isUpdated) {\n            alert.success('User updated successfully')\n            dispatch(loadUser());\n\n            history.push('/me')\n\n            dispatch({\n                type: UPDATE_PROFILE_RESET\n            })\n        }\n\n    }, [dispatch, alert, error, history, isUpdated])\n\n    const submitHandler = (e) => {\n        e.preventDefault();\n\n        const formData = new FormData();\n        formData.set('name', name);\n        formData.set('email', email);\n        formData.set('avatar', avatar);\n\n        dispatch(updateProfile(formData))\n    }\n\n    const onChange = e => {\n        const reader = new FileReader();\n\n        reader.onload = () => {\n            if (reader.readyState === 2) {\n                setAvatarPreview(reader.result)\n                setAvatar(reader.result)\n            }\n        }\n\n        reader.readAsDataURL(e.target.files[0])\n\n    }\n    return (\n        <Fragment>\n            <MetaData title={'Update Profile'} />\n\n            <div className=\"row wrapper\">\n                <div className=\"col-10 col-lg-5\">\n                    <form className=\"shadow-lg\" onSubmit={submitHandler} encType='multipart/form-data'>\n                        <h1 className=\"mt-2 mb-5\">Update Profile</h1>\n\n                        <div className=\"form-group\">\n                            <label htmlFor=\"email_field\">Name</label>\n                            <input\n                                type=\"name\"\n                                id=\"name_field\"\n                                className=\"form-control\"\n                                name='name'\n                                value={name}\n                                onChange={(e) => setName(e.target.value)}\n                            />\n                        </div>\n\n                        <div className=\"form-group\">\n                            <label htmlFor=\"email_field\">Email</label>\n                            <input\n                                type=\"email\"\n                                id=\"email_field\"\n                                className=\"form-control\"\n                                name='email'\n                                value={email}\n                                onChange={(e) => setEmail(e.target.value)}\n                            />\n                        </div>\n\n                        <div className='form-group'>\n                            <label htmlFor='avatar_upload'>Avatar</label>\n                            <div className='d-flex align-items-center'>\n                                <div>\n                                    <figure className='avatar mr-3 item-rtl'>\n                                        <img\n                                            src={avatarPreview}\n                                            className='rounded-circle'\n                                            alt='Avatar Preview'\n                                        />\n                                    </figure>\n                                </div>\n                                <div className='custom-file'>\n                                    <input\n                                        type='file'\n                                        name='avatar'\n                                        className='custom-file-input'\n                                        id='customFile'\n                                        accept='image/*'\n                                        onChange={onChange}\n                                    />\n                                    <label className='custom-file-label' htmlFor='customFile'>\n                                        Choose Avatar\n                                </label>\n                                </div>\n                            </div>\n                        </div>\n\n                        <button type=\"submit\" className=\"btn update-btn btn-block mt-4 mb-3\" disabled={loading ? true : false} >Update</button>\n                    </form>\n                </div>\n            </div>\n        </Fragment>\n    )\n}\n\nexport default UpdateProfile\n","/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/user/Profile.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/cart/OrderSuccess.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/user/UpdatePassword.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/order/ListOrders.js",["524"],"import React, { Fragment, useEffect } from \"react\";\nimport { Link } from \"react-router-dom\";\nimport { MDBDataTable } from \"mdbreact\";\n\nimport MetaData from \"../layout/MetaData\";\nimport Loader from \"../layout/Loader\";\n\nimport { useAlert } from \"react-alert\";\nimport { useDispatch, useSelector } from \"react-redux\";\nimport { myOrders, clearErrors } from \"../../actions/orderActions\";\n\nconst ListOrders = () => {\n  const alert = useAlert();\n  const dispatch = useDispatch();\n\n  const { loading, error, orders } = useSelector((state) => state.myOrders);\n\n  useEffect(() => {\n    dispatch(myOrders());\n\n    if (error) {\n      alert.error(error);\n      dispatch(clearErrors());\n    }\n  }, [dispatch, alert, error]);\n\n  const setOrders = () => {\n    const data = {\n      columns: [\n        {\n          label: \"Order ID\",\n          field: \"id\",\n          sort: \"asc\",\n        },\n        {\n          label: \"Num of Items\",\n          field: \"numOfItems\",\n          sort: \"asc\",\n        },\n        {\n          label: \"Amount\",\n          field: \"amount\",\n          sort: \"asc\",\n        },\n        {\n          label: \"Status\",\n          field: \"status\",\n          sort: \"asc\",\n        },\n        {\n          label: \"Actions\",\n          field: \"actions\",\n          sort: \"asc\",\n        },\n      ],\n      rows: [],\n    };\n\n    orders.forEach((order) => {\n      data.rows.push({\n        id: order._id,\n        numOfItems: order.orderItems.length,\n        amount: `$${order.totalPrice}`,\n        status:\n          order.orderStatus &&\n          String(order.orderStatus).includes(\"Delivered\") ? (\n            <p style={{ color: \"green\" }}>{order.orderStatus}</p>\n          ) : (\n            <p style={{ color: \"red\" }}>{order.orderStatus}</p>\n          ),\n        actions: (\n          <Link to={`/order/${order._id}`} className=\"btn btn-primary\">\n            <i className=\"fa fa-eye\"></i>\n          </Link>\n        ),\n      });\n    });\n\n    return data;\n  };\n\n  return (\n    <div className=\"container\">\n      <MetaData title={\"My Orders\"} />\n\n      <h1 className=\"my-5 text-center\">\n        <b>My Orders</b>\n      </h1>\n\n      {loading ? (\n        <Loader />\n      ) : (\n        <MDBDataTable\n          data={setOrders()}\n          className=\"px-3 \"\n          bordered\n          striped\n          hover\n        />\n      )}\n    </div>\n  );\n};\n\nexport default ListOrders;\n","/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/user/ForgotPassword.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/admin/NewProduct.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/user/UserPage.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/user/NewPassword.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/admin/ProcessOrder.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/admin/OrdersList.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/admin/UpdateProduct.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/admin/ProductReviews.js",["525"],"import React, { Fragment, useState, useEffect } from 'react'\nimport { MDBDataTable } from 'mdbreact'\n\nimport MetaData from '../layout/MetaData'\nimport Loader from '../layout/Loader'\nimport Sidebar from './Sidebar'\n\nimport { useAlert } from 'react-alert'\nimport { useDispatch, useSelector } from 'react-redux'\nimport { getProductReviews, deleteReview, clearErrors } from '../../actions/productActions'\nimport { DELETE_REVIEW_RESET } from '../../constants/productConstants'\n\nconst ProductReviews = () => {\n\n\tconst [productId, setProductId] = useState('')\n\n\tconst alert = useAlert();\n\tconst dispatch = useDispatch();\n\n\tconst { error, reviews } = useSelector(state => state.productReviews);\n\tconst { isDeleted, error: deleteError } = useSelector(state => state.review)\n\n\tuseEffect(() => {\n\n\t\tif (error) {\n\t\t\talert.error(error);\n\t\t\tdispatch(clearErrors())\n\t\t}\n\n\t\tif (deleteError) {\n\t\t\talert.error(deleteError);\n\t\t\tdispatch(clearErrors())\n\t\t}\n\n\t\tif (productId !== '') {\n\t\t\tdispatch(getProductReviews(productId))\n\t\t}\n\n\t\tif (isDeleted) {\n\t\t\talert.success('Review deleted successfully');\n\t\t\tdispatch({ type: DELETE_REVIEW_RESET })\n\t\t}\n\n\n\n\t}, [dispatch, alert, error, productId, isDeleted, deleteError])\n\n\tconst deleteReviewHandler = (id) => {\n\t\tdispatch(deleteReview(id, productId))\n\t}\n\n\tconst submitHandler = (e) => {\n\t\te.preventDefault();\n\t\tdispatch(getProductReviews(productId))\n\t}\n\n\tconst setReviews = () => {\n\t\tconst data = {\n\t\t\tcolumns: [\n\t\t\t\t{\n\t\t\t\t\tlabel: 'Review ID',\n\t\t\t\t\tfield: 'id',\n\t\t\t\t\tsort: 'asc'\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tlabel: 'Rating',\n\t\t\t\t\tfield: 'rating',\n\t\t\t\t\tsort: 'asc'\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tlabel: 'Comment',\n\t\t\t\t\tfield: 'comment',\n\t\t\t\t\tsort: 'asc'\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tlabel: 'User',\n\t\t\t\t\tfield: 'user',\n\t\t\t\t\tsort: 'asc'\n\t\t\t\t},\n\t\t\t\t{\n\t\t\t\t\tlabel: 'Actions',\n\t\t\t\t\tfield: 'actions',\n\t\t\t\t},\n\t\t\t],\n\t\t\trows: []\n\t\t}\n\n\t\treviews.forEach(review => {\n\t\t\tdata.rows.push({\n\t\t\t\tid: review._id,\n\t\t\t\trating: review.rating,\n\t\t\t\tcomment: review.comment,\n\t\t\t\tuser: review.name,\n\n\t\t\t\tactions:\n\t\t\t\t\t<button className=\"btn btn-danger py-1 px-2 ml-2\" onClick={() => deleteReviewHandler(review._id)}>\n\t\t\t\t\t\t<i className=\"fa fa-trash\"></i>\n\t\t\t\t\t</button>\n\t\t\t})\n\t\t})\n\n\t\treturn data;\n\t}\n\n\treturn (\n\t\t<Fragment>\n\t\t\t<MetaData title={'Product Reviews'} />\n\t\t\t<div className=\"row mt-5\">\n\t\t\t\t<div className=\"col-12 col-md-2 mt-4\">\n\t\t\t\t\t<Sidebar />\n\t\t\t\t</div>\n\n\t\t\t\t<div className=\"col-12 col-md-10 mt-5\">\n\t\t\t\t\t<Fragment>\n\t\t\t\t\t\t<div className=\"row justify-content-center mt-5\">\n\t\t\t\t\t\t\t<div className=\"col-5\">\n\t\t\t\t\t\t\t\t<form onSubmit={submitHandler}>\n\t\t\t\t\t\t\t\t\t<div className=\"form-group\">\n\t\t\t\t\t\t\t\t\t\t<label htmlFor=\"productId_field\">Enter Product ID</label>\n\t\t\t\t\t\t\t\t\t\t<input\n\t\t\t\t\t\t\t\t\t\t\ttype=\"text\"\n\t\t\t\t\t\t\t\t\t\t\tid=\"productId_field\"\n\t\t\t\t\t\t\t\t\t\t\tclassName=\"form-control\"\n\t\t\t\t\t\t\t\t\t\t\tvalue={productId}\n\t\t\t\t\t\t\t\t\t\t\tonChange={(e) => setProductId(e.target.value)}\n\t\t\t\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t\t\t\t<button\n\t\t\t\t\t\t\t\t\t\tid=\"search_button\"\n\t\t\t\t\t\t\t\t\t\ttype=\"submit\"\n\t\t\t\t\t\t\t\t\t\tclassName=\"btn btn-primary btn-block py-2\"\n\t\t\t\t\t\t\t\t\t>\n\t\t\t\t\t\t\t\t\t\tSEARCH\n\t\t\t\t\t\t\t\t\t</button>\n\t\t\t\t\t\t\t\t</ form>\n\t\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t</div>\n\n\t\t\t\t\t\t{reviews && reviews.length > 0 ? (\n\t\t\t\t\t\t\t<MDBDataTable\n\t\t\t\t\t\t\t\tdata={setReviews()}\n\t\t\t\t\t\t\t\tclassName=\"px-3\"\n\t\t\t\t\t\t\t\tbordered\n\t\t\t\t\t\t\t\tstriped\n\t\t\t\t\t\t\t\thover\n\t\t\t\t\t\t\t/>\n\t\t\t\t\t\t) : (\n\t\t\t\t\t\t\t<p className=\"mt-5 text-center\">No Reviews.</p>\n\t\t\t\t\t\t)}\n\n\n\t\t\t\t\t</Fragment>\n\t\t\t\t</div>\n\t\t\t</div>\n\n\t\t</Fragment>\n\t)\n}\n\nexport default ProductReviews\n","/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/admin/ProductsList.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/admin/Dashboard.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/admin/UpdateUser.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/admin/UsersList.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/admin/CatagoryList.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/layout/Footer.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/cart/ConfirmOrder.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/cart/Shipping.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/reducers/cartReducers.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/actions/userActions.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/reducers/productReducers.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/reducers/userReducers.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/reducers/orderReducers.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/reducers/categoryReducer.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/admin/NewCategory.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/route/ProtectedRoute.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/layout/MetaData.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/layout/CategorySection.js",["526"],"import React from \"react\";\r\nimport { useSelector } from \"react-redux\";\r\nimport { Link } from \"react-router-dom\";\r\nimport \"../../App1.css\";\r\nconst CategorySection = () => {\r\n  const { category } = useSelector((state) => state.category);\r\n  return (\r\n    <div class=\"category-area-start category-style-one mt-100 position-relative\">\r\n      <div style={{ width: \"80%\", marginLeft: \"auto\", marginRight: \"auto\" }}>\r\n        <div class=\"row\">\r\n          <div class=\"col-lg-12\">\r\n            <div class=\"section-head-style-one\">\r\n              <h2>What do you looking for ?</h2>\r\n              <p>We have variety of catagories available</p>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <div class=\"row\">\r\n          {category.map((category, idx) => {\r\n            return (\r\n              <div\r\n                class=\"col-lg-2 col-md-3 col-sm-6 category-box-alpha shadow-sm\"\r\n                style={{ borderRadius: \"20px\" }}\r\n              >\r\n                <div class=\"category-icon\">\r\n                  <Link to={`/search/${category.name}`}>\r\n                    <img src={category.images[0].url} />\r\n                  </Link>\r\n                </div>\r\n                <h5 class=\"category-title\">\r\n                  <Link to={`/search/${category.name}`}>{category.name}</Link>\r\n                </h5>\r\n              </div>\r\n            );\r\n          })}\r\n        </div>\r\n      </div>\r\n    </div>\r\n  );\r\n};\r\n\r\nexport default CategorySection;\r\n","/Volumes/Programming/Final-Year/project-shopit/frontend/src/actions/categoryActions.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/product/Product.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/layout/Banner.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/actions/productActions.js",[],"/Volumes/Programming/Final-Year/project-shopit/frontend/src/components/layout/Features.js",[],"/Volumes/Programming/Final-Year/project-shopi